data Integer is end

data Maybe a is Just a | Nothing end

x :: Integer
is
  (\ x -> x) 3
end

jabber :: Maybe Integer -> Integer
is
  \ x -> case x of
         { Nothing -> 0
         ; Just y  -> y
         }
end

jub :: Integer
is
  jabber Nothing
end

jerb :: Maybe Integer -> Maybe Integer -> Integer
is
  \ m ->
    \ x -> case x of
           { Nothing -> 0
           ; Just y  -> y
           }
end

jreb :: Maybe Integer -> Integer
is
  \ m ->
    (\ x -> case x of
            { Nothing -> 0
            ; Just y  -> y
            }) m
end
